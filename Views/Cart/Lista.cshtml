@model List<partner_aluro.Models.CartItem>

@{
    ViewData["Title"] = "Zamowienia";
    Layout = "~/Views/Shared/_Dashboard.cshtml";
}

<div class="row">
    <div class="col">ID</div>
    <div class="col">Product ID</div>
    <div class="col">Ilosc</div>
    <div class="col">CartId</div>
</div>
@for (var i = 0; i < @Model.Count(); i++)
{
    <div class="row">
        <div class="col">@Model[i].Id</div>
        <div class="col">@Model[i].Product</div>
        <div class="col">@Model[i].Quantity</div>
        <div class="col">@Model[i].CartId</div>
    </div>
}